@model IEnumerable<WebManagementPortal.EF.License>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CourseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Grade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentsCapacity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RecLog)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contract.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CourseCatalog.GroupName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CourseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Grade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentsCapacity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RecLog)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contract.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CourseCatalog.GroupName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
