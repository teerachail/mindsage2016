@model IEnumerable<WebManagementPortal.EF.CourseCatalog>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GroupName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Grade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Advertisements)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SideName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PriceUSD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Series)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FullDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DescriptionImageUrl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RecLog)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GroupName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Grade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Advertisements)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SideName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PriceUSD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Series)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FullDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DescriptionImageUrl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RecLog)
        </td>
        <td>
            @Html.ActionLink("View", "Details", new { id=item.Id })
        </td>
    </tr>
}

</table>
